<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd"
	logicalFilePath="2025/11/12.xml">

	<changeSet id="1762966257-1" author="openk9">

		<dropForeignKeyConstraint
			baseTableName="route_security"
			constraintName="fk_route_security_tenant"/>

		<addForeignKeyConstraint
			baseTableName="route_security"
			baseColumnNames="tenant_id"
			constraintName="fk_route_security_tenant"

			referencedTableName="tenant"
			referencedColumnNames="tenant_id"
			onDelete="CASCADE"
		/>

	</changeSet>

	<changeSet id="1762966257-2" author="openk9">

		<dropForeignKeyConstraint
			baseTableName="api_key"
			constraintName="fk_api_key_tenant"/>

		<addForeignKeyConstraint
			baseTableName="api_key"
			baseColumnNames="tenant_id"
			constraintName="fk_api_key_tenant"

			referencedTableName="tenant"
			referencedColumnNames="tenant_id"
			onDelete="CASCADE"
		/>

	</changeSet>

</databaseChangeLog>
